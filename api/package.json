{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typeorm": "node -r ts-node/register -r tsconfig-paths/register node_modules/typeorm/cli.js",
    "typeorm:dev": "npm run typeorm -- --dataSource ./src/infra/database/datasource.ts",
    "typeorm:test": "DOTENV_CONFIG_PATH=.env.test npm run typeorm -- --dataSource ./src/infra/database/datasource.ts",
    "fixtures": "fixtures-ts-node-commonjs",
    "pre:test": "npm run typeorm:test schema:drop && npm run typeorm:test migration:run && npm run db:test:load-fixtures",
    "db:test:load-fixtures": "DOTENV_CONFIG_PATH=.env.test npm run fixtures -- load ./test/utils/fixtures --dataSource ./src/infra/database/datasource.ts --require tsconfig-paths/register",
    "test": "npm run pre:test && DOTENV_CONFIG_PATH=.env.test jest --config jest.config.ts",
    "test:integration": "npm run pre:test && DOTENV_CONFIG_PATH=.env.test jest --testRegex 'test/integration/.*\\.routes\\.spec\\.ts$' --config jest.config.ts",
    "test:e2e": "npm run pre:test && DOTENV_CONFIG_PATH=.env.test jest --testRegex 'test/e2e/.*\\.e2e-spec\\.ts$' --config test/e2e/jest-e2e.json",
    "test:watch": "jest --watch --config jest.config.ts",
    "test:cov": "npm run pre:test && DOTENV_CONFIG_PATH=.env.test jest --coverage --config jest.config.ts",
    "test:unit": "jest --testRegex 'test/unit/.*\\.spec\\.ts$' --config jest.config.ts",
    "badge:coverage": "coverage-badges --in=coverage/coverage-summary.json && node scripts/move-badge.cjs",
    "swagger:generate": "ts-node -r tsconfig-paths/register scripts/generate-swagger.ts"
  },
  "dependencies": {
    "@mantine/dropzone": "^8.1.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/bcrypt": "^5.0.2",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cloudinary": "^2.7.0",
    "multer": "^2.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sqlite3": "^5.1.7",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.24",
    "typeorm-transactional": "^0.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.1.3",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.13",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "coverage-badges-cli": "^2.1.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm-fixtures-cli": "^4.1.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
